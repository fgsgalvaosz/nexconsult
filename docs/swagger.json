{
    "schemes": [
        "http",
        "https"
    ],
    "swagger": "2.0",
    "info": {
        "description": "API REST para consulta automatizada no Sintegra do Maranhão com resolução automática de reCAPTCHA v2 e extração estruturada de dados.",
        "title": "NexConsult Sintegra MA API",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "name": "NexConsult",
            "url": "https://nexconsult.com",
            "email": "contato@nexconsult.com"
        },
        "license": {
            "name": "Apache 2.0",
            "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
        },
        "version": "1.0.0"
    },
    "host": "localhost:3000",
    "basePath": "/",
    "paths": {
        "/": {
            "get": {
                "description": "Exibe informações básicas sobre a API, endpoints disponíveis e exemplos de uso",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "System"
                ],
                "summary": "API Information",
                "responses": {
                    "200": {
                        "description": "Informações da API",
                        "schema": {
                            "$ref": "#/definitions/models.StandardResponse"
                        }
                    }
                }
            }
        },
        "/api/v1/sintegra/consultar": {
            "post": {
                "description": "Executa consulta automatizada no Sintegra MA com resolução de CAPTCHA e extração de dados estruturados",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Sintegra"
                ],
                "summary": "Consultar CNPJ no Sintegra MA",
                "parameters": [
                    {
                        "description": "Dados do CNPJ para consulta",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/models.SintegraRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Consulta realizada com sucesso",
                        "schema": {
                            "allOf": [
                                {
                                    "$ref": "#/definitions/models.StandardResponse"
                                },
                                {
                                    "type": "object",
                                    "properties": {
                                        "data": {
                                            "$ref": "#/definitions/models.SintegraResponse"
                                        }
                                    }
                                }
                            ]
                        }
                    },
                    "400": {
                        "description": "Requisição inválida",
                        "schema": {
                            "$ref": "#/definitions/models.StandardResponse"
                        }
                    },
                    "429": {
                        "description": "Rate limit excedido",
                        "schema": {
                            "$ref": "#/definitions/models.StandardResponse"
                        }
                    },
                    "500": {
                        "description": "Erro interno do servidor",
                        "schema": {
                            "$ref": "#/definitions/models.StandardResponse"
                        }
                    }
                }
            }
        },
        "/api/v1/sintegra/consultar-lote": {
            "post": {
                "description": "Executa consulta automatizada em lote no Sintegra MA para múltiplos CNPJs simultaneamente",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Sintegra"
                ],
                "summary": "Consultar múltiplos CNPJs em lote",
                "parameters": [
                    {
                        "description": "Lista de CNPJs para consulta em lote (máximo 10)",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/models.BatchSintegraRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Consulta em lote realizada com sucesso",
                        "schema": {
                            "allOf": [
                                {
                                    "$ref": "#/definitions/models.StandardResponse"
                                },
                                {
                                    "type": "object",
                                    "properties": {
                                        "data": {
                                            "$ref": "#/definitions/models.BatchSintegraResponse"
                                        }
                                    }
                                }
                            ]
                        }
                    },
                    "400": {
                        "description": "Requisição inválida ou CNPJs inválidos",
                        "schema": {
                            "$ref": "#/definitions/models.StandardResponse"
                        }
                    },
                    "429": {
                        "description": "Rate limit excedido",
                        "schema": {
                            "$ref": "#/definitions/models.StandardResponse"
                        }
                    },
                    "500": {
                        "description": "Erro interno do servidor",
                        "schema": {
                            "$ref": "#/definitions/models.StandardResponse"
                        }
                    }
                }
            }
        },
        "/api/v1/sintegra/status": {
            "post": {
                "description": "Verifica se uma consulta de CNPJ está em andamento ou concluída",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Sintegra"
                ],
                "summary": "Verifica o status de uma consulta de CNPJ",
                "parameters": [
                    {
                        "description": "CNPJ para verificar status",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/models.StatusRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Status da consulta",
                        "schema": {
                            "allOf": [
                                {
                                    "$ref": "#/definitions/models.StandardResponse"
                                },
                                {
                                    "type": "object",
                                    "properties": {
                                        "data": {
                                            "$ref": "#/definitions/models.StatusResponse"
                                        }
                                    }
                                }
                            ]
                        }
                    },
                    "400": {
                        "description": "Requisição inválida",
                        "schema": {
                            "$ref": "#/definitions/models.StandardResponse"
                        }
                    },
                    "500": {
                        "description": "Erro interno do servidor",
                        "schema": {
                            "$ref": "#/definitions/models.StandardResponse"
                        }
                    }
                }
            }
        },
        "/docs": {
            "get": {
                "description": "Documentação detalhada dos endpoints disponíveis, formatos de resposta e exemplos",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "System"
                ],
                "summary": "API Documentation",
                "responses": {
                    "200": {
                        "description": "Documentação completa da API",
                        "schema": {
                            "$ref": "#/definitions/models.StandardResponse"
                        }
                    }
                }
            }
        },
        "/health": {
            "get": {
                "description": "Verifica o status de saúde da API, tempo de atividade e versão",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "System"
                ],
                "summary": "Health Check",
                "responses": {
                    "200": {
                        "description": "API funcionando corretamente",
                        "schema": {
                            "allOf": [
                                {
                                    "$ref": "#/definitions/models.StandardResponse"
                                },
                                {
                                    "type": "object",
                                    "properties": {
                                        "data": {
                                            "$ref": "#/definitions/models.HealthResponse"
                                        }
                                    }
                                }
                            ]
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "models.BatchSintegraRequest": {
            "description": "Estrutura de dados para requisição de consulta em lote de CNPJs",
            "type": "object",
            "required": [
                "cnpjs"
            ],
            "properties": {
                "cnpjs": {
                    "description": "Lista de CNPJs para consulta (cada CNPJ deve ter 14 dígitos numéricos)\n@example [\"38139407000177\",\"27394162000170\"]",
                    "type": "array",
                    "maxItems": 10,
                    "minItems": 1,
                    "items": {
                        "type": "string"
                    },
                    "example": [
                        "[\"38139407000177\"",
                        "\"27394162000170\"]"
                    ]
                }
            }
        },
        "models.BatchSintegraResponse": {
            "description": "Dados estruturados retornados pela consulta em lote no Sintegra MA",
            "type": "object",
            "properties": {
                "error_count": {
                    "description": "Número de consultas com erro\n@example 1",
                    "type": "integer",
                    "example": 1
                },
                "errors": {
                    "description": "Erros das consultas que falharam\nMapa onde a chave é o CNPJ e o valor é a mensagem de erro",
                    "type": "object",
                    "additionalProperties": {
                        "type": "string"
                    }
                },
                "execution_time": {
                    "description": "Tempo total de execução do lote (formato: \"45s\" ou \"2m30s\")\n@example \"1m15s\"",
                    "type": "string",
                    "example": "1m15s"
                },
                "invalid_cnpjs": {
                    "description": "Lista de CNPJs inválidos que não foram processados\n@example [\"123456\"]",
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "example": [
                        "[\"123456\"]"
                    ]
                },
                "results": {
                    "description": "Resultados das consultas bem-sucedidas\nMapa onde a chave é o CNPJ e o valor é o resultado da consulta",
                    "type": "object",
                    "additionalProperties": {
                        "$ref": "#/definitions/models.SintegraResponse"
                    }
                },
                "success_count": {
                    "description": "Número de consultas bem-sucedidas\n@example 2",
                    "type": "integer",
                    "example": 2
                },
                "timestamp": {
                    "description": "Timestamp da consulta\n@example \"2025-08-25T17:25:30.468715-03:00\"",
                    "type": "string",
                    "example": "2025-08-25T17:25:30.468715-03:00"
                },
                "total": {
                    "description": "Número total de CNPJs processados\n@example 3",
                    "type": "integer",
                    "example": 3
                }
            }
        },
        "models.CNAEData": {
            "type": "object",
            "properties": {
                "codigo": {
                    "type": "string"
                },
                "descricao": {
                    "type": "string"
                }
            }
        },
        "models.EnderecoData": {
            "type": "object",
            "properties": {
                "bairro": {
                    "type": "string"
                },
                "cep": {
                    "type": "string"
                },
                "complemento": {
                    "type": "string"
                },
                "ddd": {
                    "type": "string"
                },
                "logradouro": {
                    "type": "string"
                },
                "municipio": {
                    "type": "string"
                },
                "numero": {
                    "type": "string"
                },
                "telefone": {
                    "type": "string"
                },
                "uf": {
                    "type": "string"
                }
            }
        },
        "models.ErrorDetails": {
            "type": "object",
            "properties": {
                "code": {
                    "description": "Código do erro\n@example \"INVALID_CNPJ\"",
                    "type": "string",
                    "example": "INVALID_CNPJ"
                },
                "details": {
                    "description": "Detalhes adicionais do erro"
                },
                "message": {
                    "description": "Mensagem do erro\n@example \"CNPJ inválido. Deve conter 14 dígitos\"",
                    "type": "string",
                    "example": "CNPJ inválido. Deve conter 14 dígitos"
                }
            }
        },
        "models.HealthResponse": {
            "type": "object",
            "properties": {
                "service": {
                    "type": "string"
                },
                "status": {
                    "type": "string"
                },
                "uptime": {
                    "type": "string"
                },
                "version": {
                    "type": "string"
                }
            }
        },
        "models.ObrigacoesData": {
            "type": "object",
            "properties": {
                "cte_a_partir_de": {
                    "type": "string"
                },
                "edf_a_partir_de": {
                    "type": "string"
                },
                "nfe_a_partir_de": {
                    "type": "string"
                }
            }
        },
        "models.ResponseMeta": {
            "type": "object",
            "properties": {
                "execution_time": {
                    "description": "Tempo de execução da operação\n@example \"1.234s\"",
                    "type": "string",
                    "example": "1.234s"
                },
                "request_id": {
                    "description": "ID da requisição para rastreamento\n@example \"req_123456789\"",
                    "type": "string",
                    "example": "req_123456789"
                },
                "timestamp": {
                    "description": "Timestamp da resposta em formato ISO 8601\n@example \"2025-08-25T17:25:30.468715-03:00\"",
                    "type": "string",
                    "example": "2025-08-25T17:25:30.468715-03:00"
                },
                "version": {
                    "description": "Versão da API\n@example \"v1\"",
                    "type": "string",
                    "example": "v1"
                }
            }
        },
        "models.SintegraData": {
            "type": "object",
            "properties": {
                "cgc": {
                    "type": "string"
                },
                "cnae_principal": {
                    "type": "string"
                },
                "cnae_secundarios": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/models.CNAEData"
                    }
                },
                "data_consulta": {
                    "type": "string"
                },
                "data_situacao_cadastral": {
                    "type": "string"
                },
                "endereco": {
                    "$ref": "#/definitions/models.EnderecoData"
                },
                "inscricao_estadual": {
                    "type": "string"
                },
                "numero_consulta": {
                    "type": "string"
                },
                "obrigacoes": {
                    "$ref": "#/definitions/models.ObrigacoesData"
                },
                "observacao": {
                    "type": "string"
                },
                "razao_social": {
                    "type": "string"
                },
                "regime_apuracao": {
                    "type": "string"
                },
                "situacao_cadastral": {
                    "type": "string"
                }
            }
        },
        "models.SintegraRequest": {
            "description": "Estrutura de dados para requisição de consulta CNPJ",
            "type": "object",
            "required": [
                "cnpj"
            ],
            "properties": {
                "cnpj": {
                    "description": "CNPJ da empresa para consulta (apenas números, 14 dígitos)\n@example \"38139407000177\"",
                    "type": "string",
                    "example": "38139407000177"
                }
            }
        },
        "models.SintegraResponse": {
            "description": "Dados estruturados retornados pela consulta no Sintegra MA",
            "type": "object",
            "properties": {
                "captcha_solved": {
                    "description": "Indica se o CAPTCHA foi resolvido automaticamente\n@example true",
                    "type": "boolean",
                    "example": true
                },
                "cnpj": {
                    "description": "CNPJ consultado\n@example \"38139407000177\"",
                    "type": "string",
                    "example": "38139407000177"
                },
                "data": {
                    "description": "Dados estruturados da empresa",
                    "allOf": [
                        {
                            "$ref": "#/definitions/models.SintegraData"
                        }
                    ]
                },
                "execution_time": {
                    "description": "Tempo de execução da consulta (formato: \"45s\" ou \"2m30s\")\n@example \"45s\"",
                    "type": "string",
                    "example": "45s"
                },
                "status": {
                    "description": "Status da consulta (sucesso, erro_captcha, erro_extracao)\n@example \"sucesso\"",
                    "type": "string",
                    "example": "sucesso"
                },
                "timestamp": {
                    "description": "Timestamp da consulta\n@example \"2025-08-25T17:25:30.468715-03:00\"",
                    "type": "string",
                    "example": "2025-08-25T17:25:30.468715-03:00"
                },
                "url": {
                    "description": "URL da página de resultado no Sintegra\n@example \"https://sistemas1.sefaz.ma.gov.br/sintegra/...\"",
                    "type": "string",
                    "example": "https://sistemas1.sefaz.ma.gov.br/sintegra/..."
                }
            }
        },
        "models.StandardResponse": {
            "description": "Estrutura padrão unificada de resposta para todos os endpoints",
            "type": "object",
            "properties": {
                "data": {
                    "description": "Dados retornados pela operação (apenas quando status = success)"
                },
                "error": {
                    "description": "Detalhes do erro (apenas quando status = error)",
                    "allOf": [
                        {
                            "$ref": "#/definitions/models.ErrorDetails"
                        }
                    ]
                },
                "message": {
                    "description": "Mensagem descritiva da operação\n@example \"Consulta realizada com sucesso\"",
                    "type": "string",
                    "example": "Consulta realizada com sucesso"
                },
                "meta": {
                    "description": "Metadados da resposta",
                    "allOf": [
                        {
                            "$ref": "#/definitions/models.ResponseMeta"
                        }
                    ]
                },
                "status": {
                    "description": "Status da operação (success, error, warning, info)\n@example \"success\"",
                    "type": "string",
                    "example": "success"
                }
            }
        },
        "models.StatusRequest": {
            "type": "object",
            "required": [
                "cnpj"
            ],
            "properties": {
                "cnpj": {
                    "description": "CNPJ da empresa para verificar status",
                    "type": "string"
                }
            }
        },
        "models.StatusResponse": {
            "type": "object",
            "properties": {
                "cnpj": {
                    "description": "CNPJ consultado",
                    "type": "string"
                },
                "mensagem": {
                    "description": "Mensagem adicional sobre o status",
                    "type": "string"
                },
                "status": {
                    "description": "Status da consulta (em_andamento, concluida, erro, nao_encontrada)",
                    "type": "string"
                },
                "tempo_estimado": {
                    "description": "Tempo estimado para conclusão (em segundos), apenas quando status=em_andamento",
                    "type": "integer"
                },
                "timestamp": {
                    "description": "Timestamp da verificação",
                    "type": "string"
                }
            }
        }
    }
}